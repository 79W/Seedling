/*

Darcula color scheme from the JetBrains family of IDEs

*/

.hljs-style-darcula .hljs {
    display: block;
    overflow-x: auto;
    padding: 0.5em;
    background: #2b2b2b;
}

.hljs-style-darcula .hljs {
    color: #bababa;
}

.hljs-style-darcula .hljs-strong,
.hljs-style-darcula .hljs-emphasis {
    color: #a8a8a2;
}

.hljs-style-darcula .hljs-bullet,
.hljs-style-darcula .hljs-quote,
.hljs-style-darcula .hljs-link,
.hljs-style-darcula .hljs-number,
.hljs-style-darcula .hljs-regexp,
.hljs-style-darcula .hljs-literal {
    color: #6896ba;
}

.hljs-style-darcula .hljs-code
.hljs-style-darcula .hljs-selector-class {
    color: #a6e22e;
}

.hljs-style-darcula .hljs-emphasis {
    font-style: italic;
}

.hljs-style-darcula .hljs-keyword,
.hljs-style-darcula .hljs-selector-tag,
.hljs-style-darcula .hljs-section,
.hljs-style-darcula .hljs-attribute,
.hljs-style-darcula .hljs-name,
.hljs-style-darcula .hljs-variable {
    color: #cb7832;
}

.hljs-style-darcula .hljs-params {
    color: #b9b9b9;
}

.hljs-style-darcula .hljs-string {
    color: #6a8759;
}

.hljs-style-darcula .hljs-subst,
.hljs-style-darcula .hljs-type,
.hljs-style-darcula .hljs-built_in,
.hljs-style-darcula .hljs-builtin-name,
.hljs-style-darcula .hljs-symbol,
.hljs-style-darcula .hljs-selector-id,
.hljs-style-darcula .hljs-selector-attr,
.hljs-style-darcula .hljs-selector-pseudo,
.hljs-style-darcula .hljs-template-tag,
.hljs-style-darcula .hljs-template-variable,
.hljs-style-darcula .hljs-addition {
    color: #e0c46c;
}

.hljs-style-darcula .hljs-comment,
.hljs-style-darcula .hljs-deletion,
.hljs-style-darcula .hljs-meta {
    color: #7f7f7f;
}
